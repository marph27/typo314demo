        <target state="final">Diese Site wurde noch nicht konfiguriert.</target>
      </trans-unit>
      <trans-unit id="overview.addSiteConfiguration" approved="yes">
        <source>Add new site configuration for this site</source>
        <target state="final">Neue Site-Konfiguration für diese Seite hinzufügen</target>
      </trans-unit>
      <trans-unit id="overview.editSiteConfiguration" approved="yes">
        <source>Edit site configuration</source>
        <target state="final">Site-Konfiguration bearbeiten</target>
