        <source>Duplicated root pages</source>
        <target state="final">Duplizierte Root-Seiten</target>
      </trans-unit>
      <trans-unit id="overview.duplicatedRootPage.message" approved="yes">
        <source>The page with ID "%1s" is used in the following site configurations:</source>
